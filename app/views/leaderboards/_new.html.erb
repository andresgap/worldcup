<div class="modal-header">
  <button type="button" class="close" data-dismiss="modal">&times;</button>
  <h4 class="modal-title">New Leaderboard</h4>
</div>
<div>
  <%= form_for(leaderboard, url: leaderboards_path(leaderboard), remote: true) do |f| %>
    <%= render 'form', f: f, leaderboard: leaderboard %>
  <% end %>
</div>
